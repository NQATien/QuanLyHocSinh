@model NamHocVM

<h1>Thêm năm học mới</h1>

<form method="post" asp-action="Create" asp-controller="NamHoc" enctype="multipart/form-data">

    <div class="form-group">
        <label>Năm học: </label>
        <input asp-for="NamHocName" class="form-control" />
  
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
