@model BoMonVM

<h1>Thêm bộ môn mới</h1>

<form method="post" asp-action="Create" asp-controller="BoMon" enctype="multipart/form-data">

    <div class="form-group">
        <label>Bộ môn: </label>
        <input asp-for="BoMonName" class="form-control" />
  
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
